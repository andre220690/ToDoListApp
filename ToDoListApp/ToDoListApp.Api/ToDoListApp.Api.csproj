    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>b5a0c42c-e99a-4d90-8249-21b042e92739</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
